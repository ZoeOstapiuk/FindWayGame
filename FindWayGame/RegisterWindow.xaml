<Window x:Class="FindWayGame.RegisterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FindWayGame"
        mc:Ignorable="d"
        Title="Register" Height="300" Width="300" ResizeMode="NoResize">
    <DockPanel LastChildFill="True">
        <TextBox x:Name="txtNickname" DockPanel.Dock="Top" Margin="10"/>
        <TextBox x:Name="txtPassword" DockPanel.Dock="Top" Margin="10"/>
        <Button x:Name="btnAdd" Content="I'M READY!" Click="btnAdd_Click"/>
    </DockPanel>
</Window>
