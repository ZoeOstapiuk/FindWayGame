<Window x:Class="FindWayGame.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FindWayGame"
        mc:Ignorable="d"
        Title="Find way game" Height="350" Width="525">
    <DockPanel LastChildFill="True">
        <Grid DockPanel.Dock="Top">
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Label Grid.Column="0" x:Name="lblNickname" Margin="10" HorizontalAlignment="Left" />
        </Grid>
        <Grid x:Name="gameGrid">
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
                <RowDefinition/>
            </Grid.RowDefinitions>
            <Button x:Name="btn00" Grid.Row="0" Grid.Column="0" Content="{StaticResource leaf}" Background="LightBlue" Click="btnClick" Template="{StaticResource waterPlateButton}" />
            <Button x:Name="btn10" Grid.Row="1" Grid.Column="0" Content="{StaticResource leaf}" Background="LightBlue" Click="btnClick" Template="{StaticResource waterPlateButton}" />
            <Button x:Name="btn20" Grid.Row="2" Grid.Column="0" Content="{StaticResource leaf}" Background="LightBlue" Click="btnClick" Template="{StaticResource waterPlateButton}" />
            <Button x:Name="btn30" Grid.Row="3" Grid.Column="0" Content="{StaticResource leaf}" Background="LightBlue" Click="btnClick" Template="{StaticResource waterPlateButton}" />
            <Button x:Name="btn40" Grid.Row="4" Grid.Column="0" Content="{StaticResource leaf}" Background="LightBlue" Click="btnClick" Template="{StaticResource waterPlateButton}" />
            <Button x:Name="btn01" Grid.Row="0" Grid.Column="1" Content="{StaticResource leaf}" Background="LightBlue" Click="btnClick" Template="{StaticResource waterPlateButton}" />
            <Button x:Name="btn11" Grid.Row="1" Grid.Column="1" Content="{StaticResource leaf}" Background="LightBlue" Click="btnClick" Template="{StaticResource waterPlateButton}" />
            <Button x:Name="btn21" Grid.Row="2" Grid.Column="1" Content="{StaticResource leaf}" Background="LightBlue" Click="btnClick" Template="{StaticResource waterPlateButton}" />
            <Button x:Name="btn31" Grid.Row="3" Grid.Column="1" Content="{StaticResource leaf}" Background="LightBlue" Click="btnClick" Template="{StaticResource waterPlateButton}" />
            <Button x:Name="btn41" Grid.Row="4" Grid.Column="1" Content="{StaticResource leaf}" Background="LightBlue" Click="btnClick" Template="{StaticResource waterPlateButton}" />
            <Button x:Name="btn02" Grid.Row="0" Grid.Column="2" Content="{StaticResource leaf}" Background="LightBlue" Click="btnClick" Template="{StaticResource waterPlateButton}" />
