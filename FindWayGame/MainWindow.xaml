            <Button x:Name="btn03" Grid.Row="0" Grid.Column="3" Content="{StaticResource leaf}" Background="LightBlue" Click="btnClick" Template="{StaticResource waterPlateButton}" />
            <Button x:Name="btn13" Grid.Row="1" Grid.Column="3" Content="{StaticResource leaf}" Background="LightBlue" Click="btnClick" Template="{StaticResource waterPlateButton}" />
